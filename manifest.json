{
    "manifest_version": 2,
    "name": "Histograph",
    "version": "0.1",
    "content_scripts" : [
    {
        "matches" : [
            "<all_urls>"
        ],
        "js" : ["content.js", "vis-network.js"]
    }  
    ],
    "background": {
        "page": "background.html"
    },
    "browser_action": {
        "default_icon": "pic.png"
    },
    "permissions": [
        "history", "tabs"
    ]
}